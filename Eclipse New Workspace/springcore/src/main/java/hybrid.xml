<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<context:component-scan
		base-package="com/te/springcore"></context:component-scan>


	<bean class="com.te.springcore.bean.hybrid.Car" id="car">
		<property name="carId" value="10" />
	</bean>

	<bean class="com.te.springcore.bean.hybrid.Car" id="car1">
		<property name="carId" value="20" />
	</bean>

	<bean class="com.te.springcore.bean.hybrid.Engine" id="engine">
		<property name="engineName" value="superengine" />
	</bean>
	
		<bean class="com.te.springcore.bean.hybrid.Engine" id="engine1">
		<property name="engineName" value="normalengine" />
	</bean>
</beans>